{
  "version": 2,
  "framework": "vite",
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://www.memeroyalegame.com",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "memeroyalegame.com"
        }
      ]
    }
  ],
  "env": {
    "NODE_VERSION": "20"
  }
}